{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src/",
    "paths": {
      "@/components/*": ["components/*"],
      "@/hooks/*": ["hooks/*"],
      "@/screens/*": ["screens/*"],
      "@/router/*": ["router/*"],
      "@/layout/*": ["layout/*"],
      "@/shared/*": ["shared/*"],
      "@/store/*": ["store/*"],
      "@/configs/*": ["configs/*"],
      "@/services/*": ["services/*"],
      "@/ui/*": ["ui/*"],
      "@/constants/*": ["constants/*"],
      "@/utils/*": ["utils/*"],
      "@/api/*": ["api/*"]
//      "@/components/*": ["components/*"]
//      "@/components/*": ["components/*"]
    },
  },
  "include": [
    "src"
  ]
}
